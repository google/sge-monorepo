CT_CONFIG_VERSION="3"
CT_EXPERIMENTAL=y
CT_ARCH_X86=y
CT_ARCH_64=y
CT_CANADIAN=y
CT_HOST="x86_64-w64-mingw32"
CT_KERNEL_LINUX=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_FOR_TARGET=y
CT_LIBC_GLIBC=y
CT_GLIBC_V_2_27=y
CT_CC_LANG_CXX=y
CT_DEBUG_LTRACE=y
CT_DEBUG_STRACE=y
CT_COMP_TOOLS_LIBTOOL=y
CT_COMP_TOOLS_MAKE=y
CT_WORK_DIR="${CT_TOP_DIR}/.build"
CT_PREFIX_DIR="${CT_TOP_DIR}/build"
CT_LOCAL_TARBALLS_DIR="${CT_TOP_DIR}/src"
CT_SAVE_TARBALLS=y
CT_REMOVE_DOCS=y
CT_STRIP_HOST_TOOLCHAIN_EXECUTABLES=y
CT_TARGET_CFLAGS="-gz=none"
